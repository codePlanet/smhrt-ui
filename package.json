  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@types/jest": "24.0.5",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "@types/react-router-dom": "^4.3.1",
    "css-loader": "^3.0.0",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-scripts": "3.0.1",
    "sass-loader": "^7.1.0",
    "source-map-explorer": "^1.7.0",
    "style-loader": "^0.23.1",
    "typescript": "3.3.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
